



CREATE DATABASE tienda;
USE tienda;


CREATE TABLE `productos` (
  `id` int AUTO_INCREMENT PRIMARY KEY,
  `nombre` varchar(100) DEFAULT NULL,
  `precio` decimal(10,2) DEFAULT NULL
);


INSERT INTO `productos` (`id`, `nombre`, `precio`) VALUES
